{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Greip Changelog",
  "colors": {
    "primary": "#008CFF",
    "light": "#008CFF",
    "dark": "#008CFF"
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude"]
  },
  "favicon": "/favicon-1.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Changelog",
        "pages": ["api", "dashboard"]
      }
    ],
    "global": {}
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "appearance": {
