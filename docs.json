    }
  },
  "footer": {
    "socials": {
      "linkedin": "https://www.linkedin.com/company/greip",
      "github": "https://github.com/Greipio",
      "website": "https://greip.io",
      "x": "https://x.com/Greipio",
      "slack": "https://join.slack.com/t/greip-community/shared_invite/zt-2sxdyaaj8-418mMcVlekO6sNhKOAGWpg"
